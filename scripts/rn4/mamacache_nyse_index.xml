<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2000/10/XMLSchema-instance">

   <!-- Parameter Groups for Specific MamaSenderIds -->
   <DefineParameterGroup>
      <Name>MamaSenderIdNyseIndexP</Name>
      <Parameter>
         <Name>MamaSenderId</Name>
         <Value>552</Value>
      </Parameter>
   </DefineParameterGroup>
   <DefineParameterGroup>
      <Name>MamaSenderIdNyseIndexS</Name>
      <Parameter>
         <Name>MamaSenderId</Name>
         <Value>5520</Value>
      </Parameter>
   </DefineParameterGroup>
   <!-- END - Parameter Groups for Specific MamaSenderIds -->

   <Feed>
      <Name>mamacache_nyse_index</Name>
      <Parameter>
         <Name>SubFeeds</Name>
         <Value>mamacache_gif</Value>
	 <Value>mamacache_russelltickxdp</Value>
      </Parameter>
      <Parameter>
         <Name>NamedThreads</Name>
         <Value>thread_mamacache_gif</Value>
	 <Value>thread_mamacache_russelltickxdp</Value>
      </Parameter>
      <Parameter>
         <Name>MamaSymbolNamespacePerSubFeed</Name>
         <Value>true</Value>
      </Parameter>
      <!-- Publishing Parameters -->
      <Parameter>
         <Name>MamaPublisherTransportName</Name>
         <Value>mamacache_nyse_index</Value>
      </Parameter>
      <Parameter>
         <Name>MamaPropertiesFileName</Name>
         <Value>mama.properties.mamacache_nyse_index_feeds2</Value>
      </Parameter>
      <!-- END - Publishing Parameters -->
      <!-- FAST Publishing -->
      <Parameter>
         <Name>MamaUseFastForRecords</Name>
         <Value>true</Value>
      </Parameter>
      <Parameter>
         <Name>MamaUseFastForSimpleDeltas</Name>
         <Value>true</Value>
      </Parameter>
      <Parameter>
         <Name>MamaSubscriberHandlerType</Name>
         <Value>same_fids</Value>
      </Parameter>
      <Parameter>
         <Name>MamaFastPrecision</Name>
         <Value>6</Value>
      </Parameter>
      <Parameter>
         <Name>MamaPublishAppDataType</Name>
         <Value>false</Value>
      </Parameter>
      <!-- END - FAST Publishing -->
      <!-- Host-Specific Settings -->
      <pny2sfmct23a>
         <UseParameterGroup name="MamaCacheFtPrimary"/>
         <UseParameterGroup name="MamaSenderIdNyseIndexP"/>
      </pny2sfmct23a>
      <pny2sfmct23b>
         <UseParameterGroup name="MamaCacheFtSecondary"/>
         <UseParameterGroup name="MamaSenderIdNyseIndexS"/>
      </pny2sfmct23b>
      <!-- END - Host-Specific Settings -->
   </Feed>

   <Feed>
      <Name>mamacache_gif_russelltickxdp</Name>
      <Parameter>
         <Name>SubFeeds</Name>
         <Value>mamacache_gif</Value>
         <Value>mamacache_russelltickxdp</Value>
      </Parameter>
      <Parameter>
         <Name>NamedThreads</Name>
         <Value>thread_mamacache_gif</Value>
         <Value>thread_mamacache_russelltickxdp</Value>
      </Parameter>
      <Parameter>
         <Name>MamaSymbolNamespacePerSubFeed</Name>
         <Value>true</Value>
      </Parameter>
      <!-- Publishing Parameters -->
      <Parameter>
         <Name>MamaPublisherTransportName</Name>
         <Value>mamacache_nyse_index</Value>
      </Parameter>
      <Parameter>
         <Name>MamaPropertiesFileName</Name>
         <Value>mama.properties.mamacache_nyse_index_feeds2</Value>
      </Parameter>
      <!-- END - Publishing Parameters -->
      <!-- FAST Publishing -->
      <Parameter>
         <Name>MamaUseFastForRecords</Name>
         <Value>true</Value>
      </Parameter>
      <Parameter>
         <Name>MamaUseFastForSimpleDeltas</Name>
         <Value>true</Value>
      </Parameter>
      <Parameter>
         <Name>MamaSubscriberHandlerType</Name>
         <Value>same_fids</Value>
      </Parameter>
      <Parameter>
         <Name>MamaFastPrecision</Name>
         <Value>6</Value>
      </Parameter>
      <Parameter>
         <Name>MamaPublishAppDataType</Name>
         <Value>false</Value>
      </Parameter>
      <!-- END - FAST Publishing -->
      <!-- Host-Specific Settings -->
      <pny2sfmct23a>
         <UseParameterGroup name="MamaCacheFtPrimary"/>
         <UseParameterGroup name="MamaSenderIdNyseIndexP"/>
      </pny2sfmct23a>
      <pny2sfmct23b>
         <UseParameterGroup name="MamaCacheFtSecondary"/>
         <UseParameterGroup name="MamaSenderIdNyseIndexS"/>
      </pny2sfmct23b>
      <!-- END - Host-Specific Settings -->
   </Feed>

</config>
